{
    "name": "amirhome/hello_world",
    "description": "My own laravel package",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "amirhome\\HelloWorld\\": "src/"
        }
    },
    "authors": [
        {
            "name": "amirhome",
            "email": "amir.email@yahoo.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "autoload-dev": {
        "psr-4": {
            "amirhome\\HelloWorld\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "amirhome\\HelloWorld\\HelloWorldServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/AmirHome/hello_world.git",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}

